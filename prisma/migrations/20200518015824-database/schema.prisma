generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Cep {
  id       Int        @default(autoincrement()) @id
  cep      String?
  Cep_data Cep_data[]
}

model Cep_data {
  bairro      String?
  cep         String? @unique
  cep_id      Int?
  complemento String?
  gia         String?
  id          Int     @default(autoincrement()) @id
  localidade  String?
  logradouro  String?
  uf          String?
  unidade     String?
  Cep         Cep?    @relation(fields: [cep_id], references: [id])
}